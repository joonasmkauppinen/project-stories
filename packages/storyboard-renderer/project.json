{
  "root": "packages/storyboard-renderer",
  "sourceRoot": "packages/storyboard-renderer/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/storyboard-renderer",
        "tsConfig": "packages/storyboard-renderer/tsconfig.lib.json",
        "project": "packages/storyboard-renderer/package.json",
        "entryFile": "packages/storyboard-renderer/src/index.ts",
        "external": ["react/jsx-runtime", "@emotion/styled/base"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/storyboard-renderer/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/storyboard-renderer/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/storyboard-renderer"],
      "options": {
        "jestConfig": "packages/storyboard-renderer/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
